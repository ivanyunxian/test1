			<!--选择器页面控件ID集合 如SQRLB#TXDZ#id_button_read_card-->
			<selectorname>
			 <hiddenids></hiddenids>
			 <disabledids></disabledids>
			 <abledids></abledids>
			</selectorname>
			<!--单元信息页面控件ID集合-->
